{
  "expo": {
    "name": "Dealm",
    "slug": "Dealm",
    "scheme": "dealm",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.dealm",
      "buildNumber": "1.0.0",
      "supportsTablet": false
    },
    "android": {
      "package": "com.dealm",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ff8e71"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "themeColor": "#ff8e71"
    },
    "description": "Dealm - Share your thoughts with people",
    "githubUrl": "https://github.com/Adoobdoob71/Dealm_Mobile"
  }
}
